record(ai, "$(P)$(R):HV_RBV") {
  field(DESC, "HV readback")
  field(DTYP, "I2C A-D Converter")
  field(INP,"#C1 S4 @12, 0") 
  field(LINR,"LINEAR")
  field(SCAN,"1 second")
  field(EGUF,"400.0")
  field(EGUL,"0.0")
  field(HOPR,"200.0")
  field(LOPR,"0.0")
  field(PREC,"1")
  field(EGU,"Volts")
}
record(ai, "$(P)$(R):HV_CUR") {
  field(DESC, "HV  current readback")
